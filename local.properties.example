# Local Properties Example
# Copy this file to local.properties and fill in your values

# SDK Paths (auto-generated by Android Studio)
sdk.dir=/path/to/your/android/sdk

# Development Configuration
# These properties are for local development only
# Do not commit actual values to version control

# API Keys for development
coinpaprika.api.key=your_development_api_key_here

# Build Configuration
build.type=debug
debug.logging=true

# Local Database Configuration
local.database.path=/path/to/local/database

# Development Server Configuration (if applicable)
dev.server.url=http://localhost:8080
dev.server.api.key=your_dev_server_api_key

# Testing Configuration
test.database.name=test_portfolio_database
test.enable.logging=true