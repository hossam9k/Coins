# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to version control

# API Configuration
COINRANKING_API_KEY=your_coinranking_api_key_here
BASE_URL=https://api.coinranking.com/v2/

# Build Configuration
DEBUG_MODE=true
ENABLE_LOGGING=true

# Security Configuration
ENABLE_SSL_PINNING=false
API_TIMEOUT_SECONDS=30

# Analytics Configuration (Optional)
ANALYTICS_API_KEY=your_analytics_key_here
CRASH_REPORTING_KEY=your_crash_reporting_key_here

# Database Configuration
DATABASE_NAME=crypto_tracker_db
CACHE_SIZE_MB=50